<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Department">
   <children>
      <AnchorPane layoutX="14.0" layoutY="5.0" prefHeight="390.0" prefWidth="586.0">
         <children>
            <TableView fx:id="departmentInfo" layoutX="100.0" layoutY="14.0" prefHeight="297.0" prefWidth="344.0">
              <columns>
                <TableColumn fx:id="colDept" prefWidth="180.0" text="Department" />
                <TableColumn fx:id="colBudget" prefWidth="161.0" text="Budget" />
              </columns>
            </TableView>
            <Button fx:id="load" layoutX="169.0" layoutY="325.0" mnemonicParsing="false" onAction="#funcLoad" text="Load Data">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="back" layoutX="325.0" layoutY="325.0" mnemonicParsing="false" onAction="#funcBack" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
