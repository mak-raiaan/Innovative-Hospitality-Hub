      </Label>
      <PasswordField fx:id="confirmpasswordTextField" layoutX="135.0" layoutY="425.0" prefHeight="25.0" prefWidth="336.0" promptText="Confirm Password" />
      <Button fx:id="registerbtn" layoutX="135.0" layoutY="467.0" mnemonicParsing="false" onAction="#RegBTNAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #FF5733;" text="Register" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="closebtn" layoutX="135.0" layoutY="511.0" mnemonicParsing="false" onAction="#closebtnAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #FF5733;" text="Close" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="RegMsg" alignment="CENTER" contentDisplay="CENTER" layoutX="142.0" layoutY="95.0" prefHeight="26.0" prefWidth="336.0" textAlignment="CENTER" textFill="#189714">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="confirmLable" alignment="CENTER" layoutX="279.0" layoutY="404.0" prefHeight="18.0" prefWidth="190.0" textFill="#da1313">
