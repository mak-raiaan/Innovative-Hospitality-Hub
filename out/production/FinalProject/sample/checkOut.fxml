<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CheckOut">
   <children>
      <AnchorPane layoutX="8.0" layoutY="13.0" prefHeight="377.0" prefWidth="583.0">
         <children>
            <Text fill="#861010" layoutX="248.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Check Out" underline="true">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Label layoutX="14.0" layoutY="48.0" text="Number:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="79.0" text="Room Number:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <ComboBox fx:id="custCombo" layoutX="110.0" layoutY="44.0" onAction="#funcCustCombo" prefWidth="150.0" />
            <TextField fx:id="room" layoutX="111.0" layoutY="75.0" />
            <Button fx:id="ok" layoutX="266.0" layoutY="44.0" mnemonicParsing="false" onAction="#funcOK" text="OK">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="checkOut" layoutX="111.0" layoutY="119.0" mnemonicParsing="false" onAction="#funcCheckOut" text="Check Out">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="back" layoutX="214.0" layoutY="119.0" mnemonicParsing="false" onAction="#funcBack" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <ImageView fitHeight="108.0" fitWidth="289.0" layoutX="318.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@picture/sixth.jpg" />
               </image>
            </ImageView>
            <Label fx:id="msglabel" layoutX="335.0" layoutY="165.0" prefHeight="17.0" prefWidth="161.0" textFill="#bf0c0c">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
