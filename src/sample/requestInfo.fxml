<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RequestInfo">
    <children>
        <AnchorPane layoutX="7.0" layoutY="7.0" prefHeight="383.0" prefWidth="714.0">
            <children>
                <TableView fx:id="requestInformation" layoutX="29.0" layoutY="14.0" prefHeight="296.0" prefWidth="671.0">
                    <columns>
                        <TableColumn fx:id="colName" prefWidth="132.0" text="Name" />
                        <TableColumn fx:id="colEmail" prefWidth="170.0" text="email" />
                        <TableColumn fx:id="colRoom" minWidth="0.0" prefWidth="84.0" text="room" />
                        <TableColumn fx:id="colDeposit" prefWidth="112.0" text="deposit" />
                        <TableColumn fx:id="colDays" prefWidth="58.0" text="days" />
                        <TableColumn fx:id="colStatus" prefWidth="114.0" text="status" />
                    </columns>
                </TableView>
                <Button fx:id="load" layoutX="337.0" layoutY="337.0" mnemonicParsing="false" onAction="#funcLoadData" text="Load Data">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="accept" layoutX="462.0" layoutY="337.0" mnemonicParsing="false" onAction="#funcAccept" prefHeight="25.0" prefWidth="90.0" text="Accept">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            <Button fx:id="backBTN" layoutX="599.0" layoutY="337.0" mnemonicParsing="false" onAction="#backAction" prefHeight="25.0" prefWidth="84.0" text="Back" />
            <Button layoutX="208.0" layoutY="337.0" mnemonicParsing="false" onAction="#rejectBtn" prefHeight="25.0" prefWidth="74.0" text="Reject" />
            <Button layoutX="89.0" layoutY="337.0" mnemonicParsing="false" onAction="#removeBtn" prefHeight="25.0" prefWidth="74.0" text="Remove" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
